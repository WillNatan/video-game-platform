<form method="post" action="{{ path('game_key_delete', {'id': game_key.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ game_key.id) }}">
    <button class="btn btn-dark mt-3 ml-3">Delete</button>
</form>
